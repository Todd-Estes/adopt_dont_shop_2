<form action="/shelters" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <p>New Shelter</p>
  <label for='shelter[name]'>Shelter name:</label><br>
  <input type='text' id='shelter[name]' name='shelter[name]'><br>
  <label for='shelter[address]'>Shelter address:</label><br>
  <input type='text' id='shelter[address]' name='shelter[address]'><br>
  <label for='shelter[city]'>Shelter city:</label><br>
  <input type='text' id='shelter[city]' name='shelter[city]'><br>
  <label for='shelter[state]'>Shelter state:</label><br>
  <input type='text' id='shelter[state]' name='shelter[state]'><br>
  <label for='shelter[zip]'>Shelter zip:</label><br>
  <input type='text' id='shelter[zip]' name='shelter[zip]'><br>
  <input type='submit' value="Create Shelter"/>
</form>
